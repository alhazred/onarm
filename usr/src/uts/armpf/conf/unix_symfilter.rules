##
## Symbol filtering rules for vmunix.
##

# Eliminate debugging symbols generated by gcc.
exc:regexp:^[.$]
exc:regexp:\.[0-9]+$

# Eliminate symbols for Dtrace SDT probe entry.
exc:regexp:^__dtrace_probe_

# Eliminate static symbols generated by statictool.
exc:regexp:^(namespace_array|drvconf_hwc_spec)$,type:object,bind:local
exc:regexp:^drvconf_ddi_prop_[0-9]+_[0-9]+,type:object,bind:local
exc:regexp:^static_sdt_probe_[0-9]+$
exc:regexp:^dacf_arg_(posta|pred)_mname_[0-9]+_[0-9]+$
exc:regexp:_depends_on$

# Eliminate internal symbols in builtin_conf.c.
exc:regexp:^__builtin_device__,type:object,bind:local

# Eliminate symbols that are not used for module relocation.
exc:type:file
exc:type:section

##
## Determine symbols to be eliminated from the symbol hash.
##
nohash:regexp:^modl(inkage|drv|misc|strmod)$,type:object,bind:local
nohash:regexp:^(fsw)$,type:object,bind:local
nohash:regexp:^(netdev_privs|cb_inet_devops)$,type:object,bind:local
nohash:regexp:^(inet_devops|inet_dev_info)$,type:object,bind:local
nohash:regexp:^(inet_(attach|info|detach))$,type:func,bind:local
