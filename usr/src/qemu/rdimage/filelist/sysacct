d none usr 755 root sys
d none usr/bin 755 root sys
d none usr/sbin 755 root bin
f none usr/sbin/sar 555 root bin
s none ../sbin/sar usr/bin/sar
d none usr/lib 755 root bin
d none usr/lib/sa 755 adm bin
f none usr/lib/sa/sa1 555 root bin
f none usr/lib/sa/sa2 555 root bin
f none usr/lib/sa/sadc 555 root bin
d none var 755 root sys
d none var/adm 755 root sys
d none var/adm/sa 775 adm sys
