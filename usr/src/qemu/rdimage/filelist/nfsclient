d none etc 755 root sys
f none etc/nfssec.conf 644 root sys

d none usr 755 root sys
d none usr/lib 755 root bin
d none usr/lib/fs 755 root sys
d none usr/lib/fs/nfs 755 root sys
f none usr/lib/fs/nfs/dfmounts 555 root bin
f none usr/lib/fs/nfs/dfshares 555 root bin
m none etc/fs/nfs/mount usr/lib/fs/nfs/mount 555 root bin
f none usr/lib/fs/nfs/showmount 555 root bin
f none usr/lib/fs/nfs/umount 555 root bin

d none usr/kernel 755 root sys
d none usr/kernel/fs 755 root sys
f none usr/kernel/fs/nfs 755 root sys
