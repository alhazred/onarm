#l none etc/init.d/sshd etc/rc2.d/S40sshd
#l none etc/init.d/sshd etc/rc0.d/K40sshd

d none etc/ssh 755 root sys
f none etc/ssh/moduli 644 root sys
f none etc/ssh/ssh_config 644 root sys
f none etc/ssh/sshd_config 644 root sys

d none usr/lib/ssh 755 root sys
f none usr/lib/ssh/sftp-server 555 root bin
f none usr/lib/ssh/ssh-http-proxy-connect 555 root bin
f none usr/lib/ssh/ssh-keysign 4555 root bin
f none usr/lib/ssh/ssh-socks5-proxy-connect 555 root bin
f none usr/lib/ssh/sshd 555 root bin

